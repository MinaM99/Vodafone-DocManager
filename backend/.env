# Port on which the backend server runs  
# Example: 5000  
BACKEND_PORT=your_backend_port_here  

# Database host IP or domain  
# Example: 10.230.85.109 or db.company.com  
DB_HOST=your_database_host_here  

# Database connection port  
# Example: 1521 for Oracle, 5432 for PostgreSQL, 3306 for MySQL  
DB_PORT=your_database_port_here  

# Name of the database  
# Example: DOCTST (Test Database)  
DB_NAME=your_database_name_here  

# Database username for authentication  
# Example: SYSADM  
DB_USER=your_database_user_here  

# Database password for authentication (DO NOT commit real credentials)  
# Store this securely using environment variables in your deployment  
# Example: WW91clNlY3VyZVBhc3N3b3JkMTIzIQ==
DB_PASSWORD=your_database_password_here  
